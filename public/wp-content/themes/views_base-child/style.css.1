/*--------------------------------------------------------------
4.0 - Navigation
--------------------------------------------------------------*/
a{color:#0088cc;text-decoration:none;}
a:hover,a:visited{color:#005580;text-decoration:underline;}
.skip-link,
.assistive-text{display:none;}
nav.main-navigation{margin-top:1px;width:100%;font-size:14px;font-size:1.4rem;background-color:#f3f3f3;background-repeat:repeat-x;background-image:-moz-linear-gradient(top,whiteSmoke 0%,#EEE 100%);background-image:-webkit-gradient(linear,left top,left bottom,color-stop(0%,whiteSmoke),color-stop(100%,#EEE));background-image:-webkit-linear-gradient(top,whiteSmoke 0%,#EEE 100%);background-image:-ms-linear-gradient(top,whiteSmoke 0%,#EEE 100%);background-image:-o-linear-gradient(top,whiteSmoke 0%,#EEE 100%);filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f5f5',endColorstr='#eeeeee',GradientType=0 );background-image:linear-gradient(top,whiteSmoke 0%,#EEE 100%);}
nav.main-navigation ul{margin:0;text-indent:0;}
